# Test Environment Variables

NODE_ENV=test
PORT=3002

# Logging (error | warn | info | debug)
LOG_LEVEL=error

# Swagger Configuration
SWAGGER_UPLOAD_MAX_SIZE=10mb
SWAGGER_CACHE_TTL=0

# Execution Configuration
EXECUTION_TIMEOUT=10000
EXECUTION_MAX_RETRIES=1

# ──────────────────────────────────────────────
# LLM Configuration
# ──────────────────────────────────────────────
# Supported providers: custom | groq | openai
# Set ONE provider — all LLM calls go through it
LLM_ENABLED=false
LLM_PROVIDER=custom

# Custom (GPT-based custom API)
CUSTOM_API_KEY=test-key
CUSTOM_MODEL=test-model

# Groq
GROQ_API_KEY=test-key
GROQ_MODEL=test-model

# OpenAI
OPENAI_API_KEY=test-key
OPENAI_MODEL=test-model

# GitHub (required for Push to GitHub / PR creation)
# GITHUB_TOKEN=ghp_your_personal_access_token_here
